<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 height="11">
	<fx:Script>
		<![CDATA[
			[Bindable]
			public var text:String = "";
		]]>
	</fx:Script>
		<s:Group width="100%">
			<s:Label text="{text}" fontSize="9" horizontalCenter="0"/>
		</s:Group>
		<s:BitmapImage width="100%" scaleMode="stretch" source="@Embed('assets/icons/rb/hLine.png')" height="100%"/>
</s:Group>
